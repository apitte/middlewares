{
  "name": "apitte/middlewares",
  "description": "Middlewares support for Apitte stack",
  "keywords": [
    "api",
    "apitte",
    "middlewares",
    "relay"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/middlewares",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "apitte/core": "^0.6.0",
    "contributte/middlewares": "^0.6.0|^0.7.0|^0.8.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.9",
    "nette/di": "~2.4.15",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "conflict": {
    "nette/di": "<2.4.15"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Middlewares\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
